{
    "indentation": {
        "character": "    "
    },
    "wrapping": {
        "maxLineLength": 140,
        "anonType": {
            "defaultWrap": "keep",
            "rules": [
                {
                    "type": "onePerLine",
                    "additionalIndent": 0,
                    "conditions": [
                        {
                            "cond": "lineLength >= n",
                            "value": 140
                        }
                    ]
                }
            ]
        },
        "functionSignature": {
            "defaultWrap": "keep",
            "rules": [
                {
                    "type": "onePerLine",
                    "additionalIndent": 0,
                    "conditions": [
                        {
                            "cond": "lineLength >= n",
                            "value": 140
                        }
                    ]
                }
            ]
        },
        "callParameter": {
            "defaultWrap": "keep"
        }
    }
}